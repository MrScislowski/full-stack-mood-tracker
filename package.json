{
  "name": "full-stack-mood-tracker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"json-server -p 3001 db.json\" \"node ./backend/dist/backend/index.js\"",
    "dev:frontend": "cd ./frontend && vite",
    "dev:backend": "cd ./backend && cross-env NODE_ENV=dev ts-node-dev index.ts",
    "dev:db": "json-server -p 3001 db.json",
    "build:frontend": "cd ./frontend && tsc -b && vite build",
    "build:backend": "cd ./backend && tsc -b",
    "build": "pnpm run build:frontend && pnpm run build:backend",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test:unit": "vitest run --dir backend",
    "test:e2e-full": "playwright test",
    "test:e2e-fast": "playwright test --project=chromium",
    "test:extensive": "pnpm run build && pnpm run test:unit && pnpm run test:e2e-full"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "child_process": "^1.0.2",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.21.0",
    "json-server": "1.0.0-beta.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.47.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/json-server": "^0.14.7",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.1.1"
  }
}
